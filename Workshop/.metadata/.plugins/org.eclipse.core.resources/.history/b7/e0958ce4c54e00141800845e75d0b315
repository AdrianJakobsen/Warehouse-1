package objects;

public class Main {

	public static void main(String[] args) {
		Robot optimusPrime = new Robot();
		optimusPrime.length = 40;
		
		Robot wallE = new Robot("yellow", 20, 10);
		//wallE.length = 5;
		orientwallEStatus(wallE);
		optimusPrime.kill(wallE);
		
		Robot rex = new Robot("bitch");
		
		
		System.out.println("rex is " + rex.color);
	}

	private static void orientwallEStatus(Robot wallE) {
		if(wallE.isAlive){
			System.out.println("wallE is Happy:)");
		}else{
		System.out.println("wallE is dead");	
		}
	}

}
