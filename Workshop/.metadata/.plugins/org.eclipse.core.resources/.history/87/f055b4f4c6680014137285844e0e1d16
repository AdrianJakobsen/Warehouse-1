package cards;

public class Card {
	private String type;
	private String value;

	public Card(String type, String value){
		this.type = type;
		this.value = value;
	}
	
	public Card(Card randomCard) {
		String.format("[%s:%s]", type,value);
	}

	public String getType(){
		return this.type;
	}
	public String getValue(){
		return this.value;
	}
	public String print(){
		String cardToPrint = String.format("[%s:%s]", type,value);
		return cardToPrint;
	}

}
