package objects;

public class Main {

	public static void main(String[] args) {
		Robot optimusPrime = new Robot();
		optimusPrime.length = 40;
		
		Robot wallE = new Robot("yellow", 20, 10);
		//wallE.length = 5;
		
		Robot rex = new Robot("bitch");
		
		System.out.println("Optimus is "+ optimusPrime.length + " long.");
		System.out.println("wallE is " + wallE.length + " long");
		System.out.println("rex is " + rex.color);
	}

}
