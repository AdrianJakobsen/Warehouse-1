package arrays;

import java.util.Scanner;

public class start {

	public static void main(String[] args) {
		String [] months = new String[]{"Januar", "februar", "Mars", "April", "Mai", "Juni", "juli"};

		Scanner scanner = new Scanner(System.in);

		System.out.println("Input a number between 0 and " + (months.length-1));

		int index = scanner.nextInt();

		if (index < months.length){
			System.out.println(months[index]);
		}else{
			System.out.println("wrong input!");
		}



		/*for (int i=months.length-1; i >= 0; i--){//går gjennom array baklengs
			System.out.println(months[i]);
		}
		for (int i=0; i < months.length; i++){//går gjenomm en array fra start til slutt
			System.out.println(months[i]);
		}//itterere gjennom en array
		 */
	}

}
