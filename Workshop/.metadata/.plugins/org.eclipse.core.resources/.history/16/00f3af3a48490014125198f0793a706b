package arrays;

import java.util.Arrays;

public class arrayCopy {

	public static void main(String[] args) {
		int[] smallInts ={1,2,3};
		int[] bigInts ={101,102,103};
		
		smallInts= bigInts;
		bigInts[2]=203;
		
		System.out.println(Arrays.toString(smallInts));
		System.out.println(Arrays.toString(bigInts));

	}

}
