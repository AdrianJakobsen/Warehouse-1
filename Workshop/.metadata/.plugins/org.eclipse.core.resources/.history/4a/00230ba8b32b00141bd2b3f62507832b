
public class Test {
	public static void main (String[] args)
	{
		// ofte brukte primitive datatyper 
6 		int age = 22; // int = heltall 
7 		double height = 182.5; // double = desimaltall 
8 		boolean isMale = true; // boolean = sant/usant 
9 		 
10 		// primitite datatyper som brukes til spesielle formål 
11 		byte enByte = 127; // lister med bytes kan f.eks. holde på filer 
12 		short bitInt = 32767; // en 16-bit integer, "int" er derimot 32 bit og bør brukes i nesten alle tilfeller 
13 		long bitInt64 = 8223372036854775807L; //  en 64-bit integer 
14 		float enFloat = 13.37f; // et desimaltall på 32 bit som brukes for å lagre store tall  
15 		char etTegn = 'a'; // holder på ett tegn 
16 
 
17 		// string er ikke primitiv, for en string er en liste med chars 
18 		String fullName = "Adrian Jakobsen"; 
19 		 
20 		// under er en "char array" (array = liste) som holder på 6 bokstaver 
21 		// man kan aksessere hvert bokstav via firstname[0], firstname[1] osv 
22 		// prøv å println(firstname[5]), hva tror du kommer ut? 
23 		// hva med println(firstname) ? 
24 		char[] firstname = { 'A', 'd', 'r', 'i', 'a', 'n' };   
25 		 
26 		String printString = "Hello there "; 
27 		 
28 		if (isMale) 
29 			printString += "male"; 
30 		else 
31 			printString += "female"; 
32 		 
33 		printString += " human. Your name is " + fullName + ", you are " + height + "cm tall and " + age + " years old."; 
34 		 
35 		System.out.println(printString); 

	}

}
