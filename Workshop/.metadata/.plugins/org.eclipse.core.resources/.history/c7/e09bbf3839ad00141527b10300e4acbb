package eiendomsSystem;

import java.util.Calendar;
import java.util.Date;
import java.util.GregorianCalendar;

public class Offer {
	private Date offerMade = new GregorianCalendar().getTime();
	private Date deadlineOffer = new GregorianCalendar();
	private float amount;
	private String name;
	private String phoneNumber;
	
	public Offer (String name, float amount, String phoneNumber, int Deadline ){
		this.name =name;
		this.amount=amount;
		this.phoneNumber=phoneNumber;
		this.deadlineOffer = offerMade.
	}
}
